{
  "name": "spaceport-container-builder",
  "description": "Build all Spaceport ML containers",
  "source": {
    "type": "GITHUB",
    "location": "https://github.com/your-username/Spaceport-Website.git",
    "buildspec": "buildspec.yml"
  },
  "artifacts": {
    "type": "NO_ARTIFACTS"
  },
  "environment": {
    "type": "LINUX_CONTAINER",
    "image": "aws/codebuild/standard:7.0",
    "computeType": "BUILD_GENERAL1_LARGE",
    "privilegedMode": true
  },
  "serviceRole": "arn:aws:iam::975050048887:role/service-role/codebuild-service-role"
} 